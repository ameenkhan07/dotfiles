# source ~/.bin/tmuxinator.zsh

# Tmux 
alias txn='tmux new -s'
alias txl='tmux ls'
alias txa='tmux a -t'
alias txlw='tmux list-window'
alias txlc='tmux list-command'
alias txrs='tmux rename-session -t'
alias txks='tmux kill-session -t'


# Tmuxinator

alias txrn='tmuxinator new'
alias txro='tmuxinator open'
alias txrs='tmuxinator start'


